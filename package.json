{
	"name": "pnpm-monorepo",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"dev": "pnpm --filter cli dev",
		"build": "pnpm -r build && rm -rf dist && pnpm --filter cli deploy dist",
		"start": "pnpm build && node dist/dist/index.js",
		"test": "vitest run --reporter verbose",
		"test:watch": "vitest watch",
		"test:coverage": "vitest run --coverage",
		"check": "biome check --write .",
		"prepare": "husky"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.11.0",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.15.29",
		"@vitest/coverage-v8": "3.1.4",
		"husky": "^9.1.7",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"vitest": "^3.1.4"
	}
}
